class Solution(object):
    def removeElement(self, nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        for i in range(len(nums)):
            if nums[i]== val:
                nums[i] = 9999999999
        
        nums.sort()
        for i in range(len(nums)):
            if nums[i]==9999999999:
                return i
                
